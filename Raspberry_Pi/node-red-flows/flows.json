    {
        "id": "c9735a50.5fa3f",
        "type": "ui_template",
        "z": "b08a64b8.356cf",
        "group": "",
        "name": "Clock and logo Toolbar",
        "order": 2,
        "width": 0,
        "height": 0,
        "format": "<script id=\"titleScript\" type=\"text/javascript\">\n    $('#clock').remove();\n    var toolbar = $('.md-toolbar-tools');\n    var div = $('<div/>');\n    var p = $('<p/ id=\"clock\">');\n    var logo = $('<div style=\"margin: 0px 0px 0px auto;\"><img style=\"display: block;margin-left: auto;margin-right: auto;width: 10%;\" src=\"https://pbs.twimg.com/profile_images/1062107712325120001/yTqNpqfm_400x400.jpg\"></div>');\n    \n    $('#titleScript').parent().hide();\n    toolbar.append(logo);\n    div.append(p);\n    div[0].style.margin = '5px 5px 5px auto';\n    toolbar.append(div);\n    \n    \n    \n    function displayTitle(lh) {\n        p.text(lh); \n    }\n    \n    function upTime() {\n        var d = new Date();\n        p.text(d.toLocaleTimeString('de-AT'));\n    }\n\n    \n\n    // Watch the payload and update the title\n    (function(scope) {\n        scope.$watch('msg.payload', function(data) {\n            displayTitle(data);\n        });\n        setInterval(upTime,1000);\n    })(scope);\n    \n    function resize_window(){\n    // resize the window to 800x600\n    window.resizeBy(-100,-100);\n    }\n \n    resize_window();\n</script>",
        "storeOutMessages": false,
        "fwdInMessages": false,
        "templateScope": "local",
        "x": 280,
        "y": 20,
        "wires": [
            []
        ]
